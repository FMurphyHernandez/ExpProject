require(MASS)

P=.5
n=10

LDA=array(list(),1)

##for(j in 1:ncol(x1))
##x[,j]=as.numeric(x[,j])


for(i in 1:n){
  x=datos[result$cluster==i,]
  d=apply(x,2,sd)
  d=(d!=0)
  x=x[,d]
  x.scaled=scale(x[,-1])
  y=cbind(x[,1],x.scaled)
  y=data.frame(y)
  colnames(y)[1]='EXP'
  LDA[[i]]=lda(formula=EXP~.,data=y, prior=c(P,1-P))
  print(class(LDA[[i]]))
}
length(LDA)
for(i in 1:n)
print(class(LDA[[i]]))
